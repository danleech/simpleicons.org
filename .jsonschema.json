  "title": "Simple Icons",
  "definitions": {
    "brand": {
      "$id": "#brand",
      "description": "A single brand",
      "type": "object",
      "required": ["title", "hex", "source"],
      "properties": {
        "title": {
          "$ref": "#/definitions/title"
        },
        "slug": {
